<script src="https://cdn.jsdelivr.net/npm/three@0.150.1/build/three.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/globe.gl"></script>
<script src="https://cdn.jsdelivr.net/npm/topojson@3"></script>
<script>
    const globe = Globe()(document.getElementById('globeViz'))
      .globeImageUrl(null)
      .backgroundColor('#010a33')
      .showAtmosphere(true)
      .atmosphereColor('purple')
      .atmosphereAltitude(0.35)
      .hexPolygonResolution(3)
      .hexPolygonMargin(0.4)
      .hexPolygonUseDots(true)
      .hexPolygonColor(() => 'rgb(91, 42, 154)')
      .hexPolygonsTransitionDuration(0)
      .customThreeObject((d) => {
        if (!d.isBranch) return null;

        const group = new THREE.Group();

          const geometry = new THREE.SphereGeometry(1.5, 16, 16);
    const material = new THREE.MeshBasicMaterial({
      color: 0xffffff,
      transparent: true,
      opacity: 0.7
    });

    const glow = new THREE.Mesh(geometry, material);
    group.add(glow);

    const outerGlowGeometry = new THREE.SphereGeometry(4, 16, 16);
    const outerGlowMaterial = new THREE.MeshBasicMaterial({
      color: 0x9b59b6,
      transparent: true,
      opacity: 0.2,
      depthWrite: false
    });
    const outerGlow = new THREE.Mesh(outerGlowGeometry, outerGlowMaterial);
    group.add(outerGlow);

        return group;
      })
      .customThreeObjectUpdate((obj, d) => {
        if (!d.isBranch) return;
        const { lat, lng } = d;
        const phi = (90 - lat) * (Math.PI / 180);
        const theta = (180 - lng) * (Math.PI / 180);
        const radius = globe.getGlobeRadius ? globe.getGlobeRadius() : 100;

        obj.position.x = radius * Math.sin(phi) * Math.cos(theta);
        obj.position.y = radius * Math.cos(phi);
        obj.position.z = radius * Math.sin(phi) * Math.sin(theta);
      });

    globe.controls().autoRotate = true;
    globe.controls().autoRotateSpeed = 0.25;
    globe.controls().enableZoom = false;
    globe.controls().enablePan = false;
    globe.camera().position.set(150, 160, 100);

    globe.globeOffset([0,100]);
    // Load real map
    fetch('https://unpkg.com/globe.gl/example/datasets/ne_110m_admin_0_countries.geojson')
      .then(res => res.json())
      .then(countries => {
        globe.hexPolygonsData(countries.features);

        // Add glow points after polygons are loaded
        const branchLocations = [
          { lat: 43.68738, lng: -79.60803, isBranch: true },
          { lat: 45.49639, lng: -73.70202, isBranch: true },
          { lat: 49.20017, lng: -122.91169, isBranch: true },
