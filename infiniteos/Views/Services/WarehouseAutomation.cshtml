<script>
    // Array of fact data
    const facts = [
        {
            title: "60% of US Warehouses Will Adopt AI by 2027",
            content: "By 2027, 60% of warehouses are expected to use AI to handle rising e-commerce demands.",
            source: "Palo Alto Networks"
        },
        {
            title: "Automation Reduces Warehouse Labor Costs by 20%",
                content: "AI-driven automation can save 20% on labor costs by streamlining tasks like picking and packing.",
            source: "Gartner"
        },
        {
            title: "94% of Staffing Firms Plan to Use AI in Five Years",
                content: "Nearly all staffing firms (94%) aim to integrate AI, including for automation support, within five years.",
            source: "IBM Security"
        },
        {
            title: "75% of Frontline Workers Are Dissatisfied with Current Tech",
                content: "Three-quarters of frontline workers, including warehouse staff, find existing technology inadequate.",
            source: "Forrester Research"
        }
    ];

    let currentIndex = 0;

    // Get DOM elements
    const factCard = document.getElementById('fact-card');
    const cardTitle = document.getElementById('card-title');
    const cardContent = document.getElementById('card-content');
    const cardSource = document.getElementById('card-source');
    const prevButton = document.getElementById('prev-button');
    const nextButton = document.getElementById('next-button');
    const paginationDots = document.getElementById('pagination-dots');

    /**
     * Updates the content of the fact card based on the current index.
     */
    function updateCardContent() {
        const currentFact = facts[currentIndex];
        cardTitle.textContent = currentFact.title;
        cardContent.textContent = currentFact.content;
        cardSource.textContent = currentFact.source;
        updateDots();
    }

    /**
     * Generates and updates the pagination dots.
     */
    function renderDots() {
        paginationDots.innerHTML = ''; // Clear existing dots
        facts.forEach((_, index) => {
            const dot = document.createElement('div');
            dot.classList.add('dot');
            if (index === currentIndex) {
                dot.classList.add('active');
            }
            dot.addEventListener('click', () => {
                currentIndex = index;
                updateCardContent();
            });
            paginationDots.appendChild(dot);
        });
    }

    /**
     * Updates the active state of the pagination dots.
     */
    function updateDots() {
        const dots = paginationDots.children;
        Array.from(dots).forEach((dot, index) => {
            if (index === currentIndex) {
                dot.classList.add('active');
            } else {
                dot.classList.remove('active');
            }
        });
    }

    // Event listener for the "previous" button
    prevButton.addEventListener('click', () => {
        currentIndex = (currentIndex - 1 + facts.length) % facts.length; // Loop back to end
        updateCardContent();
    });

    // Event listener for the "next" button
    nextButton.addEventListener('click', () => {
        currentIndex = (currentIndex + 1) % facts.length; // Loop back to start
        updateCardContent();
    });

    // Initial setup on page load
    window.onload = () => {
        renderDots(); // Render dots first
        updateCardContent(); // Then update card content
    };
</script>