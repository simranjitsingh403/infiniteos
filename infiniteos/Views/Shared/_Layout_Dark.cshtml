    <style>
        /* Custom styles to override or enhance Tailwind */
        body {
            font-family: 'Inter', sans-serif;
            background-color: #1a202c; /* Equivalent to bg-gray-900 */
            color: #e2e8f0; /* Equivalent to text-gray-100 */
            margin: 0;
            padding: 0;
            overflow: hidden; /* Hide body scrollbar as scroll will be in a specific container */
            position: relative;
        }

        /* Ensure smooth scrolling for custom scroll behavior */
        html {
            scroll-behavior: smooth; /* For clicking nav items */
        }

        /* Hide default scrollbar for a cleaner look */
        #scrollContainer::-webkit-scrollbar {
            display: none; /* For Chrome, Safari, Opera */
        }

        #scrollContainer {
            -ms-overflow-style: none; /* IE and Edge */
            scrollbar-width: none; /* Firefox */
        }

        /* Styles for the overall circular navigation container (Left Side) */
        .circular-nav-container {
            position: fixed;
            left: 0;
            top: 50%;
            transform: translateY(-50%);
            width: 250px;
            height: 500px;
            overflow: hidden;
            border-radius: 0 250px 250px 0;
            background: radial-gradient(circle at 70% 50%, #3a3b4e 0%, #2a2b3c 50%, #1a1b2c 100%);
            box-shadow: inset 0 0 50px rgba(0,0,0,0.5), 0 0 20px rgba(0,0,0,0.3);
            display: none; /* Hidden by default on small screens */
            z-index: 110;
        }

        @@media (min-width: 768px) {
            .circular-nav-container {
                display: flex;
            }
        }

        .circular-path {
            position: absolute;
            left: -250px;
            top: 0;
            width: 500px;
            height: 500px;
            border-radius: 50%;
            background-color: rgba(0, 0, 0, 0.2);
        }

        .circular-nav-item {
            position: absolute;
            width: 80px;
            height: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
            text-align: center;
            background-color: transparent;
            border-radius: 9999px;
            cursor: pointer;
            transition: color 0.3s ease, background-color 0.3s ease, transform 0.3s ease;
            font-size: 1.1rem;
            color: #718096;
            font-weight: 500;
            z-index: 10;
        }

            .circular-nav-item:hover {
                color: #a0aec0;
            }

            .circular-nav-item.active {
                color: #fff;
                font-weight: bold;
                background-color: #8b5cf6;
                box-shadow: 0 0 10px rgba(139, 92, 246, 0.7), 0 0 20px rgba(139, 92, 246, 0.4);
                transform: scale(1.05);
            }

        .indicator {
            position: absolute;
            width: 30px;
            height: 8px;
            background-color: #8b5cf6;
            border-radius: 4px;
            transition: transform 0.3s ease;
            transform-origin: 250px 250px;
            box-shadow: 0 0 15px rgba(139, 92, 246, 0.9);
            z-index: 20;
            left: calc(250px + 200px - 15px);
            top: calc(250px - 4px);
        }

            .indicator::after {
                content: '';
                position: absolute;
                left: 100%;
                top: 50%;
                transform: translateY(-50%);
                width: 40px;
                height: 2px;
                background-color: #8b5cf6;
                border-radius: 1px;
                box-shadow: 0 0 10px rgba(139, 92, 246, 0.7);
            }

        /* Main Content Area Styles for 3D Scrolling */
        .main-content {
            margin-left: 250px; /* Space for the fixed nav */
            position: relative;
            z-index: 100;
            height: 100vh; /* Set height for scrollable container */
            display: flex; /* To contain scrollContainer */
            flex-direction: column;
            justify-content: center; /* Vertically center the scrollable area visually */
            padding: 0 2rem; /* Add horizontal padding */
        }

        @@media (max-width: 767px) {
            .main-content {
                margin-left: 0;
            }
        }

        #scrollContainer {
            width: 100%;
            height: 80vh; /* Adjust height of the scrollable viewport for content */
            overflow-y: scroll; /* Enable vertical scrolling */
            position: relative; /* Context for absolute positioning of content sections */
            perspective: 1000px; /* Defines the 3D perspective for child elements */
            overflow-x: hidden; /* Prevent horizontal scroll from content transforms */
        }

        #contentWrapper {
            position: relative;
            width: 100%;
            /* Height will be dynamically set by JS to allow scrolling */
        }

        .content-section {
            position: absolute; /* Stack sections on top of each other */
            width: 100%;
            height: 60vh; /* Visual height of each section as it passes through the viewport */
            display: flex;
            align-items: center;
            justify-content: center;
            flex-direction: column;
            padding: 2rem;
            box-sizing: border-box;
            border-radius: 0.5rem; /* rounded-lg */
            transition: opacity 0.1s linear, transform 0.1s linear; /* Fast transitions for dynamic transforms */
            backface-visibility: hidden; /* Prevent flickering during 3D transforms */
            transform-style: preserve-3d; /* Crucial for nested 3D transforms */
            pointer-events: none; /* Initially non-interactive to allow scroll */
            opacity: 0; /* Start hidden */
        }

            .content-section.active {
                pointer-events: auto; /* Make interactive when active */
                opacity: 1; /* Fully visible when active */
            }

            .content-section h3 {
                font-size: 3rem; /* text-5xl */
                font-weight: 700; /* font-bold */
                margin-bottom: 1rem; /* mb-4 */
                color: #fff; /* text-white */
            }

            .content-section p {
                font-size: 1.25rem; /* text-xl */
                line-height: 1.625; /* leading-relaxed */
                color: #cbd5e0; /* text-gray-300 */
                max-width: 48rem; /* max-w-2xl */
                text-align: center;
            }

            .content-section .detail-content {
                margin-top: 2rem; /* mt-8 */
                color: #a0aec0; /* text-gray-400 */
                text-align: left;
                width: 100%;
                max-width: 48rem; /* Match parent content width */
            }

                .content-section .detail-content ul {
                    list-style: disc;
                    list-style-position: inside;
                    margin-top: 1rem;
                }

                .content-section .detail-content li {
                    margin-bottom: 0.5rem;
                }

    </style>
</head>
